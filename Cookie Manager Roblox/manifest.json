{
    "manifest_version": 3,
    "name": "Roblox cookie Manager",
    "version": "1.0.1",
    "description": "Just an extension to make login with cookies more easy.",
   
    "action": {
      "default_popup": "index.html",
      "default_icon": "COOKIE MANAGER ROBLOX/LOGO.png",
      "type": "module"
    },
    "host_permissions": [
      "https://www.roblox.com/*",
      "https://web.roblox.com/*"
    ],
    "permissions": [
      "cookies",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "background": {}
  }
  